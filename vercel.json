{
  "buildCommand": "npm run build",
  "outputDirectory": "out",
  "framework": null,
  "redirects": [
    {
      "source": "http://centennialhillshomesforsale.com/:path*",
      "destination": "https://centennialhillshomesforsale.com/:path*",
      "permanent": true
    },
    {
      "source": "https://www.centennialhillshomesforsale.com/:path*",
      "destination": "https://centennialhillshomesforsale.com/:path*",
      "permanent": true
    },
    {
      "source": "http://www.centennialhillshomesforsale.com/:path*",
      "destination": "https://centennialhillshomesforsale.com/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=31536000; includeSubDomains"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/search",
      "dest": "/search.html"
    },
    {
      "src": "/buyers",
      "dest": "/buyers.html"
    },
    {
      "src": "/blog",
      "dest": "/blog.html"
    },
    {
      "src": "/neighborhoods",
      "dest": "/neighborhoods.html"
    },
    {
      "src": "/featured-home",
      "dest": "/featured-home.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 