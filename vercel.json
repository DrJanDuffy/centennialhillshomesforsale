{
  "buildCommand": "npm run build",
  "framework": "nextjs",
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.centennialhillshomesforsale.com"
        }
      ],
      "destination": "https://centennialhillshomesforsale.com/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=31536000; includeSubDomains"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/search",
      "destination": "/search.html"
    },
    {
      "source": "/buyers",
      "destination": "/buyers.html"
    },
    {
      "source": "/blog",
      "destination": "/blog.html"
    },
    {
      "source": "/neighborhoods",
      "destination": "/neighborhoods.html"
    },
    {
      "source": "/featured-home",
      "destination": "/featured-home.html"
    }
  ]
}
