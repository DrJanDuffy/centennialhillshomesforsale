{
  "buildCommand": "npm run build",
  "outputDirectory": "out",
  "framework": "nextjs",
  "redirects": [
    {
      "source": "http://centennialhillshomesforsale.com/:path*",
      "destination": "https://centennialhillshomesforsale.com/:path*",
      "permanent": true
    },
    {
      "source": "https://www.centennialhillshomesforsale.com/:path*",
      "destination": "https://centennialhillshomesforsale.com/:path*",
      "permanent": true
    },
    {
      "source": "http://www.centennialhillshomesforsale.com/:path*",
      "destination": "https://centennialhillshomesforsale.com/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=31536000; includeSubDomains"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/search",
      "destination": "/search.html"
    },
    {
      "source": "/buyers",
      "destination": "/buyers.html"
    },
    {
      "source": "/blog",
      "destination": "/blog.html"
    },
    {
      "source": "/neighborhoods",
      "destination": "/neighborhoods.html"
    },
    {
      "source": "/featured-home",
      "destination": "/featured-home.html"
    }
  ]
}
