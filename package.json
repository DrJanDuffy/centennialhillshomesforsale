{
  "name": "centennial-hills-homes",
  "version": "2.0.0",
  "description": "Premium Real Estate Website for Centennial Hills, Las Vegas - AI-Powered Property Search",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "lint:check": "next lint",
    "type-check": "tsc --noEmit",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env ANALYZE=true next build && open .next/analyze/client.html",
    "analyze:browser": "cross-env ANALYZE=true next build && open .next/analyze/client.html",
    "biome:check": "biome check .",
    "biome:format": "biome format --write .",
    "biome:lint": "biome lint .",
    "biome:fix": "biome check --write .",
    "vercel-build": "next build",
    "vercel-dev": "next dev",
    "clean": "rm -rf .next out dist .vercel",
    "clean:all": "npm run clean && rm -rf node_modules package-lock.json",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "deps:check": "npm outdated",
    "deps:update": "npm update",
    "preview": "vercel --env preview",
    "deploy:preview": "vercel --prod=false",
    "deploy:production": "vercel --prod",
    "test:build": "npm run build && npm run start & sleep 10 && curl -f http://localhost:3000 > /dev/null && kill %1",
    "test:vercel": "vercel --prod=false --yes",
    "optimize": "npm run biome:fix && npm run lint && npm run type-check",
    "pre-commit": "npm run optimize",
    "performance:audit": "npm run analyze && npm run build",
    "bundle:size": "npm run build && npx @next/bundle-analyzer .next/static/chunks",
    "check:all": "npm run optimize && npm run security:audit && npm run deps:check"
  },
