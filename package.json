{
  "name": "centennial-hills-homes",
  "version": "2.0.0",
  "description": "Premium Real Estate Website for Centennial Hills, Las Vegas - AI-Powered Property Search",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "biome check . --write",
    "lint:check": "biome check .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "type-check": "tsc --noEmit",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env ANALYZE=true next build && open .next/analyze/client.html",
    "analyze:browser": "cross-env ANALYZE=true next build && open .next/analyze/client.html",
    "biome:check": "biome check .",
    "biome:format": "biome format --write .",
    "biome:lint": "biome check .",
    "biome:fix": "biome check --write .",
    "vercel-build": "next build",
    "vercel-dev": "next dev",
    "test:vercel": "vercel --prod=false --yes",
    "optimize": "npm run biome:fix && npm run lint && npm run type-check",
    "pre-commit": "npm run optimize",
    "performance:audit": "npm run analyze && npm run build",
    "bundle:size": "npm run build && npx @next/bundle-analyzer .next/static/chunks",
    "check:all": "npm run optimize && npm run security:audit && npm run deps:check",
    "biome:expert": "node scripts/biome-expert-optimize.js",
    "biome:analyze": "biome check . --verbose",
    "biome:security": "biome check . --apply-unsafe",
    "biome:performance": "biome check . --apply",
    "biome:real-estate": "npm run biome:expert",
    "biome:full": "npm run biome:format && npm run biome:fix && npm run biome:analyze",
    "biome:report": "npm run biome:expert && open biome-optimization-report.json",
    "bundle:analyze": "npm run analyze && node scripts/bundle-analyzer.js",
    "bundle:report": "npm run bundle:analyze && open .next/analyze/bundle-analysis.html",
    "images:optimize": "npm run build && node scripts/image-optimizer.js",
    "sw:generate": "node scripts/generate-sw.js",
    "sw:update": "node scripts/update-sw.js",
    "performance:monitor": "npm run build && node scripts/performance-monitor.js",
    "performance:report": "npm run performance:monitor && open performance-report.html",
    "optimize:all": "npm run bundle:analyze && npm run images:optimize && npm run sw:generate && npm run performance:monitor",
    "deploy:optimized": "npm run optimize:all && git add . && git commit -m 'ðŸš€ Performance optimization complete' && git push",
    "hooks:install": "lefthook install",
    "hooks:uninstall": "lefthook uninstall",
    "hooks:run": "lefthook run",
    "hooks:run-pre-commit": "lefthook run pre-commit",
    "hooks:run-pre-push": "lefthook run pre-push",
    "git:quality": "npm run hooks:run-pre-commit && npm run hooks:run-pre-push",
    "git:setup": "npm run hooks:install && npm run optimize"
  },
